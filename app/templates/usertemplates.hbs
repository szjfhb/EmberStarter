<table class="table table-striped table-hover table-condensed text-left">
<thead>
	<tr>
		<th>Name</th>
		<th>Type</th>
		<th>Layout</th>
		<th>Owner</th>
		<th>Permission</th>
		<th>Date Created</th>
		<th>Date Modified</th>
		<th>Template Id</th>
	</tr>
</thead>
{{#each model as |template|}}
	<tr><td>{{template.name}}</td>
	<td>{{template.type}}</td>
	<td>{{template.layout}}</td>
	<td>{{template.owner}}</td>
	<td>{{template.permission}}</td>
	<td>{{template.created}}</td>
	<td>{{template.modified}}</td>
	<td>{{template.templateId}}</td></tr>
{{/each}}
</table>
